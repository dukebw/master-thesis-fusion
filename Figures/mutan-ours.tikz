        \coordinate (mbottomright) at (\mposx + 0.5cm, \mposy - 1.5cm);
        \coordinate (ntopleft) at (\nposx -2.0cm, \nposy + 1.15cm);
        \coordinate (nbottomright) at (\nposx + 0.5cm, \nposy - 0.4cm);


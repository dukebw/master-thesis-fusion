\newcommand{\outveccuboid}[2]{%
        \tikzcuboid{%
            shiftx=#1,%
            shifty=#2,%
            scale=\cuboidscale,%
            rotation=0,%
            densityx=1,%
            densityy=1,%
            densityz=1,%
            dimx=1,%
            dimy=1,%
            dimz=3,%
            linefront=black,%
            linetop=black,%
            lineright=black,%
            fillfront=red!50,%
            filltop=green!50,%
            fillright=blue!50,%
            anglex=180,%
            angley=90,%
            anglez=225,%
            scalex=1,%
            scaley=0.8,%
            scalez=0.6,%
            emphedge=N,%
        }
}

